{
  "name": "electron-app",
  "version": "0.0.0",
  "type": "module",
  "description": "vite + electron + vue",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/main/index.mjs",
  "scripts": {
    "dev": "vite",
    "debug": "cross-env VITE_ELECTRON_DEBUG=1 npm run dev",
    "build": "npm run clean && vue-tsc --noEmit && vite build",
    "release": "cross-env VITE_ELECTRON_BUILDER=1 npm run build",
    "clean": "rimraf ./dist",
    "lint": "run-s lint:eslint lint:stylelint lint:prettier",
    "lint:eslint": "eslint \"{src,electron,scripts}/**/*.{js,cjs,ts,tsx,vue}\" *.{js,cjs,ts} --fix --cache",
    "lint:stylelint": "stylelint \"{src,electron,scripts}/**/*.{css,scss,less,html}\" --fix --cache",
    "lint:prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tomjs/electron-devtools-installer": "^2.0.5",
    "vue": "^3.3.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@tomjs/commitlint": "^2.0.8",
    "@tomjs/eslint": "^1.1.4",
    "@tomjs/prettier": "^1.0.9",
    "@tomjs/stylelint": "^2.0.2",
    "@tomjs/tsconfig": "^1.1.5",
    "@tomjs/vite-plugin-electron": "^1.7.0",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "stylelint": "^16.0.2",
    "tsx": "^4.7.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^1.8.25"
  }
}
