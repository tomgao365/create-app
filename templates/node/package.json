{
  "name": "{{pkgName}}",
  "version": "0.0.0",
  "description": "node module",
  "keywords": [
    "node"
  ],
  "author": {
    "name": "{{gitUserName}}",
    "email": "{{gitUserEmail}}"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+{{gitUrl}}.git"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.2",
    "tsx": "^4.10.2",
    "type-fest": "^4.18.2",
    "vitest": "^1.6.0"
  }
}
