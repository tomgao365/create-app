  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Create App",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\dist\\index.mjs",
      "outFiles": [
        "${workspaceFolder}/**/*.js",
        "${workspaceFolder}/**/*.mjs"
      ],
      "console": "integratedTerminal",
      "preLaunchTask": "${defaultBuildTask}",
    }
  ]
}
