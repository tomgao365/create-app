{
  "name": "electron-app",
  "version": "0.0.0",
  "description": "vite + electron + vue",
  "engines": {
    "node": ">=18"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:6888/"
    }
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && vite build && npm run release",
    "release": "tsx ./scripts/release.ts",
    "clean": "rimraf ./dist",
    "lint": "run-s lint:eslint lint:stylelint lint:prettier",
    "lint:eslint": "eslint \"{src,electron,scripts}/**/*.{ts,tsx}\" *.{js,cjs,ts} --fix --cache",
    "lint:stylelint": "stylelint \"{src,electron,scripts}/**/*.{css,scss,less,html}\" --fix --cache",
    "lint:prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@tomjs/commitlint": "^2.0.3",
    "@tomjs/eslint": "^1.0.7",
    "@tomjs/prettier": "^1.0.5",
    "@tomjs/stylelint": "^1.0.5",
    "@tomjs/tsconfig": "^1.0.7",
    "@types/node": "^18.18.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/shelljs": "^0.8.15",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "electron": "^27.1.2",
    "electron-builder": "^24.6.4",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "shelljs": "^0.8.5",
    "stylelint": "^15.11.0",
    "tree-kill": "^1.2.2",
    "tsx": "^4.1.3",
    "typescript": "~5.2.2",
    "vite": "^5.0.2",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
