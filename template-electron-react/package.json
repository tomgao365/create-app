{
  "name": "electron-app",
  "version": "0.0.0",
  "type": "module",
  "description": "vite + electron + vue",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/main/index.mjs",
  "scripts": {
    "dev": "vite",
    "debug": "cross-env VITE_ELECTRON_DEBUG=1 npm run dev",
    "build": "npm run clean && vite build",
    "build:pack": "cross-env VITE_ELECTRON_BUILDER=1 npm run build",
    "clean": "rimraf ./dist",
    "lint": "run-s lint:eslint lint:stylelint lint:prettier",
    "lint:eslint": "eslint \"{src,electron,scripts}/**/*.{ts,tsx}\" *.{js,cjs,ts} --fix --cache",
    "lint:stylelint": "stylelint \"{src,electron,scripts}/**/*.{css,scss,less,html}\" --fix --cache",
    "lint:prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tomjs/electron-devtools-installer": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@tomjs/commitlint": "^2.0.5",
    "@tomjs/eslint": "^1.1.1",
    "@tomjs/prettier": "^1.0.6",
    "@tomjs/stylelint": "^1.1.1",
    "@tomjs/tsconfig": "^1.1.2",
    "@tomjs/vite-plugin-electron": "^1.6.0",
    "@types/node": "^18.19.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "stylelint": "^15.11.0",
    "tsx": "^4.6.2",
    "typescript": "~5.2.2",
    "vite": "^5.0.10",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
