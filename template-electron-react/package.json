{
  "name": "electron-app",
  "version": "0.0.0",
  "type": "module",
  "description": "vite + electron + vue",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/main/index.mjs",
  "scripts": {
    "dev": "vite",
    "debug": "cross-env VITE_ELECTRON_DEBUG=1 npm run dev",
    "build": "npm run clean && vite build",
    "release": "cross-env VITE_ELECTRON_BUILDER=1 npm run build",
    "clean": "rimraf ./dist",
    "lint": "run-s lint:eslint lint:stylelint lint:prettier",
    "lint:eslint": "eslint \"{src,electron,scripts}/**/*.{ts,tsx}\" *.{js,cjs,ts} --fix --cache",
    "lint:stylelint": "stylelint \"{src,electron,scripts}/**/*.{css,scss,less,html}\" --fix --cache",
    "lint:prettier": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@tomjs/electron-devtools-installer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@tomjs/commitlint": "^2.3.0",
    "@tomjs/eslint": "^1.5.0",
    "@tomjs/prettier": "^1.3.0",
    "@tomjs/stylelint": "^2.3.0",
    "@tomjs/tsconfig": "^1.4.0",
    "@tomjs/vite-plugin-electron": "^1.9.0",
    "@types/node": "^18.19.19",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "cross-env": "^7.0.3",
    "electron": "^28.2.4",
    "electron-builder": "^24.12.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "stylelint": "^16.2.1",
    "tsx": "^4.7.1",
    "typescript": "~5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
